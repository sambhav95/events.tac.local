@model events.tac.local.Models.OverviewList

<!-- [Category List] -->
<ul class="block-grid-md-4 block-grid-sm-3 block-grid-xs-1">
    @foreach (var navItem in Model)
    {
        <li>
            <div class="thumbnail">
                <a href="Event-List.html">
                    @navItem.Image

                </a>

                <div class="caption">
                    <h3 class="teaser-heading">@navItem.Title</h3>
                    <a href="@navItem.Url" class="btn btn-default">Read more</a>
                </div>
            </div>
        </li>
    }
</ul>